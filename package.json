{
  "name": "ppt-tracker",
  "version": "1.0.17",
  "description": "Track and analyze AI usage from Cursor IDE and Claude Code",
  "main": "index.js",
  "bin": {
    "ppt-tracker": "./bin/ai-usage-tracker.js"
  },
  "scripts": {
    "postinstall": "node scripts/install.js",
    "test": "node bin/ai-usage-tracker.js --help",
    "build": "pipenv run pyinstaller --onefile cli_tool/cli.py --distpath dist --name cli && chmod +x dist/cli",
    "build:clean": "rm -rf dist build && npm run build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "usage",
    "tracking",
    "cursor",
    "claude",
    "tokens",
    "cost",
    "analytics"
  ],
  "author": "Alex Ellman",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ellmanalex/pricepertoken-ai-coding-tracker.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "files": [
    "bin/",
    "cli_tool/",
    "shared/",
    "scripts/",
    "dist/",
    "README.md",
    "requirements.txt"
  ],
  "dependencies": {},
  "devDependencies": {}
}
